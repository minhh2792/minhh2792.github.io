  var videoTag = document.getElementById('video-player-tag');
  var playerObject = new VideoPlayer(videoObject, videoTag);

  // this can be accessed by backbone view on remove
  destroy_video_player = function (callback) {
    window.onresize = null;
    playerObject.destroy();
  }

  var initialize_video_player = function () {
    layout();
    if (!isMobile()) {
      window.onresize = layout;
      window.onunload = destroy_video_player;
    }
    playerObject.initialize();
  }
  initialize_video_player();
