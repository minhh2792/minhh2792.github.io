  <script src="https://unpkg.com/imask"></script>
  <script src="https://unpkg.com/clipboard@2/dist/clipboard.min.js"></script>

  <script>
    (function() {
      var VIDEO_DURATION = 24.482759;
      var IS_HOUR_NEEDED = VIDEO_DURATION >= 3600;
      var MAX_SECONDS = Math.floor(VIDEO_DURATION);
      var INPUT_MASK = IS_HOUR_NEEDED ? '00:00:00' : '00:00';

      var shareButton = document.querySelector('.js-share-btn');
      var shareModal = document.querySelector('.js-share-modal');
      var shareModalCloseButton = shareModal.querySelector('.js-modal-close-btn');
      var shareModalOverlay = shareModal.querySelector('.js-modal-overlay');
      var shareInput = shareModal.querySelector('.js-share-input');
      var startAtCheckbox = shareModal.querySelector('.js-start-at-checkbox');
      var startAtText = shareModal.querySelector('.js-start-at-text');
      var startAtInput = shareModal.querySelector('.js-start-at-input');
      var copyButton = shareModal.querySelector('.js-copy-btn');

      var originalShareInputValue = shareInput.value;

      function init() {
        IMask(startAtInput, {
          mask: INPUT_MASK,
        });

        bindEvents();
      }

      function bindEvents() {
        shareButton.addEventListener('click', toggleShareModal);
        shareModalCloseButton.addEventListener('click', toggleShareModal);
        shareModalOverlay.addEventListener('click', toggleShareModal);
        startAtCheckbox.addEventListener('change', toggleStartAt);
        startAtInput.addEventListener('input', handleStartAtInputEvent);
        startAtInput.addEventListener('blur', handleStartAtBlurEvent);
        startAtInput.addEventListener('keyup', stopEventPropagation);
        shareModal.querySelector('.sb-modal__dialog').addEventListener('click', stopEventPropagation);
        copyButton.addEventListener('click', handleCopyButtonClick);
      }

      function toggleShareModal(event) {
        toggleClassOnElement(shareModal, 'sb-modal--open');
      }

      function toggleStartAt(event) {
        event.stopPropagation();

        toggleClassOnElement(startAtText, 'share-modal__start-at__text--hidden');
        toggleClassOnElement(startAtInput, 'share-modal__start-at__input--visible');

        if (event.target.checked) {
          shareInput.value = prepareInputValueForShare(startAtInput.value);
        } else {
          shareInput.value = originalShareInputValue;
          startAtInput.value = INPUT_MASK;
        }
      }

      function stopEventPropagation(event) {
        // We need to make sure that the different keyboard shortcuts are not triggered when user types into input
        event.stopPropagation();
      }

      function handleStartAtInputEvent() {
        shareInput.value = prepareInputValueForShare(startAtInput.value);
      }

      function handleStartAtBlurEvent(event) {
        var seconds = convertMaskedStringToSeconds(event.target.value);
        if (seconds > MAX_SECONDS) {
          startAtInput.value = convertSecondsToMaskedString(MAX_SECONDS);
        }
      }

      function handleCopyButtonClick(event) {
        event.preventDefault();

        ClipboardJS.copy(shareInput);

        shareInput.selectionStart = 0;
        shareInput.selectionEnd = 0;
        shareInput.blur();

        copyButton.disabled = true;
        copyButton.innerHTML = 'Copied!';

        setTimeout(function() {
          copyButton.innerHTML = 'Copy';
          copyButton.disabled = false;
        }, 1000);
      }

      init();

      var convertMaskedStringToSeconds = pipe(
        split(':'),
        map(Number),
        calculateSeconds
      );

      function convertSecondsToMaskedString(seconds) {
        var hours = Math.floor(seconds / 3600);
        var minutes = Math.floor((seconds % 3600) / 60);
        var remainingSeconds = seconds % 60;

        if (IS_HOUR_NEEDED) {
          return formatNumberToMaskUnit(hours) + ':' + formatNumberToMaskUnit(minutes) + ':' + formatNumberToMaskUnit(remainingSeconds);
        }

        return formatNumberToMaskUnit(minutes) + ':' + formatNumberToMaskUnit(remainingSeconds);
      }

      var prepareInputValueForShare = pipe(
        convertMaskedStringToSeconds,
        validateSeconds,
        buildShareLink
      );

      function toggleClassOnElement(element, classname) {
        if (element.classList.contains(classname)) {
          element.classList.remove(classname);
        } else {
          element.classList.add(classname);
        }
      }

      function pipe(...fns) {
        return function(x) {
          return fns.reduce(function(y, f) {
            return f(y);
          }, x);
        }
      }

      function map(fn) {
        return function(array) {
          return array.map(fn);
        }
      }

      function replace(regex, replaceValue) {
        return function(string) {
          return string.replace(regex, replaceValue);
        }
      }

      function split(separator) {
        return function(string) {
          return string.split(separator);
        }
      }

      function calculateSeconds(arrayOfNumbers) {
        switch (arrayOfNumbers.length) {
          case 3:
            var [hours, minutes, seconds] = arrayOfNumbers;
            return hours * 3600 + minutes * 60 + seconds;
            break;
          
          case 2:
            var [minutes, seconds] = arrayOfNumbers;
            return minutes * 60 + seconds;
            break;

          default:
            return arrayOfNumbers[0];
            break;
        }
      }

      function validateSeconds(seconds) {
        if (seconds > MAX_SECONDS) {
          return MAX_SECONDS;
        }

        // should not occur
        if (seconds < 0) {
          return 0;
        }

        return seconds;
      }

      function buildShareLink(seconds) {
        var url = new URL(originalShareInputValue);
        var searchParams = new URLSearchParams({
          t: seconds,
          src: 'player-page-share'
        });
        url.search = searchParams.toString();
        return url.toString();
      }

      var formatNumberToMaskUnit = function (number) {
        return String(number).padStart(2, '0');
      };
    })();
  </script>


<script id="player-js" type="text/javascript">
  var menuButton = document.querySelector('.js-download-actions-menu-btn');
  var actionsMenu = document.querySelector('.js-download-actions-menu');

  function toggleMenu() {
    if (actionsMenu.style.display === "block") {
      actionsMenu.style.display = "none"
      document.body.onclick = null;
    } else {
      actionsMenu.style.display = "block"
      setTimeout(function () {
        document.body.onclick = function () {
          toggleMenu()
        };
      }, 0);
    }
  }

  if (menuButton) {
    menuButton.addEventListener('click', toggleMenu);
  }

  var w = 540;
  var h = 958;

  var areOverlapping = function (el1, el2) {
    var rect1 = el1.getBoundingClientRect();
    var rect2 = el2.getBoundingClientRect();
    return !(rect1.right < rect2.left || rect1.left > rect2.right || rect1.bottom < rect2.top || rect1.top > rect2.bottom);
  }

  var layout = function () {
    if (isMobile()) {
      
        var padding = 380;
      
    } else {
      
        var padding = 120;
      
    }

    var height;
    if (h > window.innerHeight - padding) {
      height = window.innerHeight - padding;
      var MIN_HEIGHT = 140;
      var minHeight = h < MIN_HEIGHT ? h : MIN_HEIGHT;
      if (height < minHeight) {
        height = minHeight;
      }
    } else {
      height = h;
    }
    var width = w / h * height;
    if (width > 800 && window.innerWidth < 1200) {
      width = 800;
    }
    document.getElementById('player-content').style.visibility = "visible";
    document.getElementById('player-content').style.maxWidth = width + "px";
    document.getElementById('filler').style.maxWidth = width + "px";
    document.getElementById('filler').style.paddingBottom = 177.40740740740742 + "%";
    document.getElementById('media-container').style.maxWidth = width + "px";

    var adContent = document.getElementById('ad-content');
    if (adContent) {
      adContent.style.display = "block";
    }
  };

